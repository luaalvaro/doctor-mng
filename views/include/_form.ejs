    <div class="form-content">
        <form action="/add" method="POST" autocomplete="off">
        <div class="form-group">
            <label for="">NOME</label>
            <input type="text" name="name">
            <span class="error-description error-name"></span>
        </div>
        <div class="form-group">
            <label for="">CRM</label>
            <input type="text" name="crm" onkeypress="maskCrm()" maxlength="9">
            <span class="error-description error-crm"></span>
        </div>
        <div class="form-group">
            <label for="">TELEFONE</label>
            <input type="text" name="telephone" onkeypress="maskTelephone()" maxlength="13">
            <span class="error-description error-telephone"></span>
        </div>
        <div class="form-group">
            <label for="">CELLPHONE</label>
            <input type="text" name="cellphone" onkeypress="maskCellphone()" maxlength="14">
            <span class="error-description error-cellphone"></span>
        </div>
        <div class="form-group">
            <label for="">CEP</label>
            <input type="text" name="cep" onfocusout="getCep()" class="" onkeypress="maskCep()" maxlength="9">
            <span class="error-description error-cep"></span>
        </div>
        <div class="form-group">
            <label for="">RUA</label>
            <input type="text" name="street">
            <span class="error-description error-street"></span>
        </div>
        <div class="form-group">
            <label for="">BAIRRO</label>
            <input type="text" name="neighborhood">
            <span class="error-description error-neighborhood"></span>
        </div>
        <div class="form-group">
            <label for="">CIDADE</label>
            <input type="text" name="city">
            <span class="error-description error-city"></span>
        </div>
        <div class="form-group">
            <label for="">ESTADO</label>
            <input type="text" name="state">
            <span class="error-description error-state"></span>
        </div>
        <div class="form-group">
            <label for="">ESPECIALIDADE</label>
            <input type="text" name="specialty" readonly>

                <% for(var i = 0; i < specialtys.length; i++) { %>  
                <a class="select-specialty"onclick="insere(this.id)" id="<%= specialtys[i].specialty %>"><%= specialtys[i].specialty %></a>
                <% } %>
            
            <span class="error-description error-specialty"></span>
        </div>
        </form>
        <button onclick="check()">CADASTRAR MÃ‰DICO</button>
    </div>